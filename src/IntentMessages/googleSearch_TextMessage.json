{
  "type": "text",
  "text": "hi ${username}, 按下面按鈕可以去 google 搜尋喔 ",
  "QuickReply": {
    "items": [
      {
        "type": "action",
        "imageUrl": "https://example.com/sushi.png",
        "action": {
          "$type": "NetCoreLineBotSDK.Models.Action.UriAction, NetCoreLineBotSDK",
          "uri": "https://www.google.com/search?q=${username}",
          "type": "uri",
          "label": "Go Google"
        }
      },
      {
        "type": "action",
        "imageUrl": "https://example.com/tempura.png",
        "action": {
          "$type": "NetCoreLineBotSDK.Models.Action.MessageAction, NetCoreLineBotSDK",
          "type": "message",
          "label": "Tempura",
          "text": "Tempura"
        }
      },
      {
        "type": "action",
        "action": {
          "$type": "NetCoreLineBotSDK.Models.Action.LocationAction, NetCoreLineBotSDK",
          "type": "location",
          "label": "Send location"
        }
      }
    ]
  }
}
