                <h4 class="modal-title">資料上傳中</h4>
            </div>
            <div class="modal-body">
                <div class="out-info">
                    <img src="images/ajax-loader.gif" id="loadingImg">

                </div>
            </div>
        </div>
    </div>
</div>

@code {
    [Parameter]
    public string Id { get; set; } = "gallery";

